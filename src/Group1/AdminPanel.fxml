<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0" prefWidth="700.0" spacing="10.0" style="-fx-background-color: #1b1b1b;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Group1.AdminController">
   <children>
      <Label text="Admin Panel" textFill="WHITE">
         <font>
            <Font name="Rockwell Condensed Bold" size="32.0" />
         </font>
         <padding>
            <Insets top="10.0" />
         </padding>
      </Label>
      <Line endX="350.0" startX="-350.0" stroke="GREY" strokeWidth="1.5" />
      <HBox alignment="CENTER" prefHeight="125.0" prefWidth="350.0" spacing="50.0">
         <children>
            <Button fx:id="wordListSelection" alignment="CENTER" mnemonicParsing="false" onAction="#handleWordListSelection" style="-fx-background-color: gray;" text="Select Word List" textFill="WHITE">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font></Button>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextArea fx:id="charFreqArea" editable="false" minHeight="-Infinity" prefHeight="300.0" prefWidth="350.0" />
            <TextArea fx:id="wordFreqArea" editable="false" minHeight="-Infinity" prefHeight="300.0" prefWidth="350.0" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#close" text="Close">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></Button>
   </children>
</VBox>
